      "standard"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "eslint": "^8.18.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.3",
    "eslint-plugin-promise": "^6.0.0",
    "mocha": "^10.0.0",
    "rollup": "^2.75.7",
    "typescript": "^4.7.4"
  },
  "engine": {
    "node": ">=12"
  }
